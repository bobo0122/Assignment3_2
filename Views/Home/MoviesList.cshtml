@{
    ViewBag.Title = "Movie List";

}
@model IEnumerable<Assignment3_2.Models.ApplicationResponse>

<div>
    <h2>Here are the submitted movies.</h2>    <table>
        <thead>
            <tr>
                <th>Category</th>
                <th>Title</th>
                <th>Year</th>
                <th>Director</th>
                <th>Rating</th>
                <th>Edited</th>
                <th>Lent To:</th>
                <th>Notes</th>
            </tr>
        </thead>
        <tbody>
           
                    @foreach (Assignment3_2.Models.ApplicationResponse y in Model)
                    {
                    <tr>
                        <td>@y.Category</td>
                        <td>@y.Title</td>
                        <td>@y.Year</td>
                        <td>@y.Director</td>
                        <td>@y.SelectRatingType</td>
                        <td>@y.Edited</td>
                        <td>@y.LentTo</td>
                        <td>@y.Notes</td>
                    </tr>
                }
              
        </tbody>
    </table>
</div>